#include <stdio.h>
#include<string.h>
int main()
{
 char username[30],password[30]; 
	printf("\nenter user name: ");
	scanf("%s",username);
	printf("\nenter PASSWORD: ");
	scanf("%s",password);	
	if(strcasecmp(username,"Admin")==0&&strcmp(password,"Admin")==0){
	    printf("login successfull\n");
	}
	else
	printf("unsuccessfull\n");
	return 0;
} 
